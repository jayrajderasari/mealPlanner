{
  "version": 2,
  "builds": [
    {
      "src": "/mealPlannerBackend/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "/mealPlannerFrontend/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/mealPlannerBackend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/mealPlannerFrontend/index.html"
    }
  ]
}
